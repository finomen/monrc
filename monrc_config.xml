<monrc>
    <monitored>
        <services>
            <service>numlock</service>
        </services>
        <runlevels>
            <runlevel>default</runlevel>
        </runlevels>
    </monitored>
    <excluded>
        <services>
            <service>sshd</service>
            <service>netmount</service>
        </services>
    </excluded>

    <disableLoggingFor>
        <services>
            <service>net.eth1</service>
            <service>netmount</service>
        </services>
    </disableLoggingFor>
</monrc>
